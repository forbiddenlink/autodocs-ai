# Anthropic API Key for Claude
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# GitHub OAuth
GITHUB_CLIENT_ID=your_github_client_id
GITHUB_CLIENT_SECRET=your_github_client_secret
GITHUB_CALLBACK_URL=http://localhost:3000/api/auth/callback/github

# Database
DATABASE_URL=postgresql://user:password@localhost:5432/autodocs_ai

# Pinecone (Vector DB)
PINECONE_API_KEY=your_pinecone_api_key
PINECONE_ENVIRONMENT=your_pinecone_environment
PINECONE_INDEX=autodocs-embeddings

# Next.js
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=generate_a_random_secret_here
NEXT_PUBLIC_BASE_URL=https://autodocs.ai
NEXT_PUBLIC_API_URL=http://localhost:3001

# CDN Configuration (optional)
# Set this to serve static assets from a CDN
# Example: https://cdn.autodocs.ai
# CDN_URL=https://your-cdn-url.com

# Optional: Tree-sitter parser
TREE_SITTER_LANGUAGES=typescript,javascript,python,java,go,rust
