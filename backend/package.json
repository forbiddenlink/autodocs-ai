{
  "name": "autodocs-backend",
  "version": "1.0.0",
  "description": "Backend API for AutoDocs AI",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "dev": "nodemon src/index.js",
    "start": "node src/index.js",
    "worker": "node src/worker.js",
    "worker:dev": "nodemon src/worker.js",
    "start:pm2": "pm2 start ecosystem.config.cjs",
    "start:pm2:prod": "pm2 start ecosystem.config.cjs --env production",
    "start:pm2:staging": "pm2 start ecosystem.config.cjs --env staging",
    "reload": "pm2 reload ecosystem.config.cjs",
    "reload:prod": "pm2 reload ecosystem.config.cjs --env production",
    "reload:staging": "pm2 reload ecosystem.config.cjs --env staging",
    "stop": "pm2 stop ecosystem.config.cjs",
    "delete": "pm2 delete ecosystem.config.cjs",
    "status": "pm2 status",
    "logs": "pm2 logs",
    "monit": "pm2 monit",
    "migrate": "node src/scripts/migrate.js",
    "migrate:rollback": "node src/scripts/rollback.js",
    "backup": "bash scripts/backup.sh",
    "restore": "bash scripts/restore.sh",
    "backup:validate": "bash scripts/validate-backup.sh",
    "lint": "eslint \"src/**/*.js\"",
    "lint:fix": "eslint \"src/**/*.js\" --fix",
    "format": "prettier --write \"src/**/*.js\"",
    "format:check": "prettier --check \"src/**/*.js\"",
    "test": "jest"
  },
  "keywords": [
    "documentation",
    "ai",
    "api"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.9.1",
    "@pinecone-database/pinecone": "^1.1.2",
    "@sentry/node": "^10.32.1",
    "@sentry/profiling-node": "^10.32.1",
    "axios": "^1.6.2",
    "bcrypt": "^5.1.1",
    "bullmq": "^5.68.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "ioredis": "^5.9.2",
    "jsonwebtoken": "^9.0.3",
    "morgan": "^1.10.0",
    "openai": "^6.21.0",
    "pg": "^8.11.3",
    "prom-client": "^15.1.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "tree-sitter": "^0.20.4",
    "tree-sitter-javascript": "^0.20.3",
    "tree-sitter-python": "^0.20.4",
    "tree-sitter-typescript": "^0.20.3",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "pm2": "^6.0.14",
    "prettier": "^3.7.4"
  }
}
